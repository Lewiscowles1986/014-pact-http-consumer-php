{
  "name": "braddle/http-consumer-pact",
  "description": "Demonstation of Pact for HTTP Consumer testing.",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Bradley",
      "email": "braddle@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Braddle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Braddle\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.23",
    "pact-foundation/pact-php": "dev-ffi-next as 10.0.0"
  },
  "require": {
    "guzzlehttp/guzzle": "6.5.8"
  },
  "config": {
    "allow-plugins": {
      "tienvx/composer-downloads-plugin": true
    }
  }
}
